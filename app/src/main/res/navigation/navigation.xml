<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation"
    app:startDestination="@id/fragmentMain">
    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.example.botik.view.fragment.FragmentMain"
        android:label="FragmentMain" >
        <action
            android:id="@+id/action_fragmentMain_to_fragmentmanto"
            app:destination="@id/fragmentmanto" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAds"
        android:name="com.example.botik.view.fragment.FragmentAds"
        android:label="FragmentAds" />
    <fragment
        android:id="@+id/fragmentCart"
        android:name="com.example.botik.view.fragment.FragmentCart"
        android:label="FragmentCart" >
        <action
            android:id="@+id/action_fragmentCart_to_fragmentAddress"
            app:destination="@id/fragmentAddress" />
    </fragment>
    <fragment
        android:id="@+id/fragmentmanto"
        android:name="com.example.botik.view.fragment.Fragmentmanto"
        android:label="Fragmentmanto" >
        <action
            android:id="@+id/action_fragmentmanto_to_fragmentDetails"
            app:destination="@id/fragmentDetails" />
    </fragment>
    <fragment
        android:id="@+id/fragmentDetails"
        android:name="com.example.botik.view.fragment.FragmentDetails"
        android:label="FragmentDetails" >
        <action
            android:id="@+id/action_fragmentDetails_to_fragmentLogin"
            app:destination="@id/fragmentLogin" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLogin"
        android:name="com.example.botik.view.fragment.FragmentLogin"
        android:label="FragmentLogin" >
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentRegister"
            app:destination="@id/fragmentRegister" />
        <action
            android:id="@+id/action_fragmentLogin_to_fragmentProfile"
            app:destination="@id/fragmentProfile" />
    </fragment>
    <fragment
        android:id="@+id/fragmentProfile"
        android:name="com.example.botik.view.fragment.FragmentProfile"
        android:label="FragmentProfile" >
        <action
            android:id="@+id/action_fragmentProfile_to_fragmentLogin"
            app:destination="@id/fragmentLogin" />
        <action
            android:id="@+id/action_fragmentProfile_to_fragmentRegister"
            app:destination="@id/fragmentRegister" />
        <action
            android:id="@+id/action_fragmentProfile_to_fragmentEdit"
            app:destination="@id/fragmentEdit" />
        <action
            android:id="@+id/action_fragmentProfile_to_fragmentOrder"
            app:destination="@id/fragmentOrder" />
        <action
            android:id="@+id/action_fragmentProfile_to_fragmentShowData"
            app:destination="@id/fragmentShowData" />
    </fragment>
    <fragment
        android:id="@+id/fragmentRegister"
        android:name="com.example.botik.view.fragment.FragmentRegister"
        android:label="FragmentRegister" >
        <action
            android:id="@+id/action_fragmentRegister_to_fragmentProfile"
            app:destination="@id/fragmentProfile" />
        <action
            android:id="@+id/action_fragmentRegister_to_fragmentLogin"
            app:destination="@id/fragmentLogin" />
    </fragment>
    <fragment
        android:id="@+id/fragmentEdit"
        android:name="com.example.botik.view.fragment.FragmentEdit"
        android:label="FragmentEdit" >
        <action
            android:id="@+id/action_fragmentEdit_to_fragmentProfile"
            app:destination="@id/fragmentProfile" />
    </fragment>
    <fragment
        android:id="@+id/fragmentOrder"
        android:name="com.example.botik.view.fragment.FragmentOrder"
        android:label="FragmentOrder" >
        <action
            android:id="@+id/action_fragmentOrder_to_fragmentProfile"
            app:destination="@id/fragmentProfile" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAddress"
        android:name="com.example.botik.view.fragment.FragmentAddress"
        android:label="FragmentAddress" >
        <action
            android:id="@+id/action_fragmentAddress_to_fragmentPay"
            app:destination="@id/fragmentPay" />
    </fragment>
    <fragment
        android:id="@+id/fragmentPay"
        android:name="com.example.botik.view.fragment.FragmentPay"
        android:label="FragmentPay" />
    <fragment
        android:id="@+id/fragmentShowData"
        android:name="com.example.botik.view.fragment.FragmentShowData"
        android:label="FragmentShowData" >
        <action
            android:id="@+id/action_fragmentShowData_to_fragmentProfile"
            app:destination="@id/fragmentProfile" />
    </fragment>
</navigation>